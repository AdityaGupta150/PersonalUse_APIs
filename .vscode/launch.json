    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Start Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\bin\\server.js"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Deploy Check",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "--trace-deprecation"
            ],
            "program": "${workspaceFolder}\\bin\\server.js"
        }
    ]
}